addi r1 1
addi r2 2
// Number of iterations here
addi r15 10

r15--
brh 0b01 endfib1
r15--
brh 0b01 endfib2

fib:add r3 r1 r2
r15--
brh 0b01 endr3

add r1 r2 r3
r15--
brh 0b01 endr1

add r2 r3 r1
r15--
brh 0b01 endr2

jmp fib

endr3:add r14 r0 r3
halt
endr1:add r14 r0 r1
halt
endr2:add r14 r0 r2
halt
endfib1:addi r14 1
halt
endfib2:addi r14 2
halt

